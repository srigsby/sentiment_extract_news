        function submitmyform(frm) {
            url = "http://realestate.money.cnn.com/syndication/?id=AACN0001&url=";
            url += encodeURIComponent("http://realestate.money.cnn.com/validate.php?");
            if(frm.search.value) {
                url += "search="+encodeURIComponent(frm.search.value+"&");
            }
            if(frm.min_price.value) {
                url += "min_price="+encodeURIComponent(frm.min_price.value+"&");
            }
            if(frm.max_price.value) {
                url += "max_price="+encodeURIComponent(frm.max_price.value+"&");
            }
            if(frm.type.value) {
                url += "type="+encodeURIComponent(frm.type.value+"&");
            }
            frm.action = url;
        }
